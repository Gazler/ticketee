<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<h1><%= link_to "Ticketee", root_path %></h1>
<nav>
  <%= link_to "Sign up", new_user_registration_path %>
</nav>

<% flash.each do |key, value| %>
  <div class='flash' id='<%= key %>'>
  <%= value %>
  </div>
<% end %>

<%= yield %>

</body>
</html>

